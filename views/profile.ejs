<%- include('header')%>
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div>
                            <img src="<%= locals.admin.image %>" class="img-thumbnail rounded mx-auto d-block m-3"
                                height="200" width="150" />
                        </div>
                        <div class="table-responsive table--no-card m-b-30">
                            <table class="table table-borderless table-striped table-earning">
                                <thead>
                                    <tr>
                                        <th>Fides</th>
                                        <th>Value</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <th>Name : </th>
                                        <th>
                                            <%= locals.admin.name %>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>Email : </th>
                                        <th>
                                            <%= locals.admin.email %>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>Gender : </th>
                                        <th>
                                            <%= locals.admin.gender %>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>Hobby : </th>
                                        <th>
                                            <%= locals.admin.hobby %>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>City Name : </th>
                                        <th>
                                            <%= locals.admin.city %>
                                        </th>
                                    </tr>
                                    <tr>
                                        <th>Message : </th>
                                        <th>
                                            <%= locals.admin.message %>
                                        </th>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>

                    <%- include('footer')%>