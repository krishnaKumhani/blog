<%- include('header')%>
    <div class="main-content">
        <div class="section__content section__content--p30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <strong>Add Admin</strong>
                            </div>
                            <% if(locals.flash.success.length> 0 && locals.flash.success) { %>
                                <div class="alert alert-success">
                                    <%= locals.flash.success %>
                                </div>
                                <% } %>
                                    <% if(locals.flash.error.length> 0 && locals.flash.error) { %>
                                        <div class="alert alert-danger">
                                            <%= locals.flash.error %>
                                        </div>
                                        <% } %>
                                            <div class="card-body card-block">
                                                <form action="/admin/review/inserreviewData" method="post"
                                                    class="form-horizontal">

                                                    <!-- last Name -->
                                                    <div class="row form-group">
                                                        <div class="col col-md-3">
                                                            <label for="text-input" class=" form-control-label">Enter
                                                                icon</label>
                                                        </div>
                                                        <div class="col-12 col-md-9">
                                                            <input type="text" id="text-input" name="icon"
                                                                placeholder="Enter Your icon" class="form-control">
                                                        </div>
                                                    </div>

                                                    <!-- Message -->
                                                    <div class="row form-group">
                                                        <div class="col col-md-3">
                                                            <label for="textarea-input"
                                                                class="form-control-label">Textarea</label>
                                                        </div>
                                                        <div class="col-12 col-md-9">
                                                            <textarea name="message" id="textarea-input" rows="3"
                                                                placeholder="Message..."
                                                                class="form-control"></textarea>
                                                        </div>
                                                    </div>

                                                    <!-- first Name -->
                                                    <div class="row form-group">
                                                        <div class="col col-md-3">
                                                            <label for="text-input" class=" form-control-label">Enter
                                                                Name</label>
                                                        </div>
                                                        <div class="col-12 col-md-9">
                                                            <input type="text" id="text-input" name="name"
                                                                placeholder="Enter Your Title" class="form-control">
                                                        </div>
                                                    </div>

                                                    <% city=["Surat","Vapi","Rajkot","Dehli"] %>
                                                        <div class="row form-group">
                                                            <div class="col col-md-3">
                                                                <label for="select" class=" form-control-label">Select
                                                                    City</label>
                                                            </div>
                                                            <div class="col-12 col-md-9">
                                                                <select name="city" id="select" class="form-control">
                                                                    <option value="0">-- select city --</option>
                                                                    <% city.map((v,i)=>{%>
                                                                        <option value="<%= v %>">
                                                                            <%= v %>
                                                                        </option>
                                                                        <%})%>
                                                                </select>
                                                            </div>
                                                        </div>


                                                        <% contr=["India","U.K.","U.S.A.","Japan"] %>
                                                            <div class="row form-group">
                                                                <div class="col col-md-3">
                                                                    <label for="select"
                                                                        class=" form-control-label">Select
                                                                        contr</label>
                                                                </div>
                                                                <div class="col-12 col-md-9">
                                                                    <select name="contr" id="select"
                                                                        class="form-control">
                                                                        <option value="0">-- select contr --</option>
                                                                        <% contr.map((v,i)=>{%>
                                                                            <option value="<%= v %>">
                                                                                <%= v %>
                                                                            </option>
                                                                            <%})%>
                                                                    </select>
                                                                </div>
                                                            </div>




                                            </div>
                                            <div class="card-footer">
                                                <button type="submit" class="btn btn-primary btn-sm">
                                                    <i class="fa fa-dot-circle-o"></i> Submit
                                                </button>
                                                <button type="reset" class="btn btn-danger btn-sm">
                                                    <i class="fa fa-ban"></i> Reset
                                                </button>
                                            </div>
                                            </form>

                        </div>

                    </div>

                </div>
                <%- include('footer')%>